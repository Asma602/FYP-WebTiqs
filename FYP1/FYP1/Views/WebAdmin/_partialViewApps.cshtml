@model IEnumerable<FYP1.Models.userApplication>


<table class="table table-bordered" id="viewDataTable">
    <thead>
        <tr>
            <th>App Name</th>
            <th>App Logo</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.appName</td>
                    <td><img src="~/logo/@item.appLogo" alt="App Logo" style="width:50px;height:50px;" /></td>
                </tr>
            }
        }
    </tbody>
</table>